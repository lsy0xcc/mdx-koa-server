<div>
  <style>
    table,
    td {
      border: 1px solid #333;
    }

    thead,
    tfoot {
      background-color: #333;
      color: #fff;
    }
  </style>
  <h1>{{name}}</h1>
  <table>
    <thead>
      <tr>
        <th>key</th>
        <th>value</th>
      </tr>
    </thead>
    <tbody>
      {{#each data}}
      <tr>
        <td>{{this.key}}</td>
        <td>{{{this.value}}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>